{
  "name": "@nordicsemiconductor/cloud-aws-firmware-ci-feature-runner-action",
  "version": "0.0.0-development",
  "description": "Uses @nordicsemiconductor/e2e-bdd-test-runner to run features files of a firmware.",
  "scripts": {
    "build": "npx ncc build run-features.ts -o build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@actions/core": "1.2.7",
    "@aws-sdk/client-iot": "3.14.0",
    "@aws-sdk/client-sts": "3.14.0",
    "@nordicsemiconductor/cloudformation-helpers": "6.0.21",
    "@nordicsemiconductor/e2e-bdd-test-runner": "13.1.24",
    "@nordicsemiconductor/firmware-ci-runner-aws": "5.3.0",
    "chalk": "4.1.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-angular": "12.1.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.19",
    "@types/node-fetch": "2.5.10",
    "@vercel/ncc": "0.28.5",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "semantic-release": "17.4.2",
    "typescript": "4.2.4"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "7.11.2"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
