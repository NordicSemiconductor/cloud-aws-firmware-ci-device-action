{
  "name": "@nordicsemiconductor/cloud-aws-firmware-ci-device-action",
  "version": "0.0.0-development",
  "description": "GitHub action that runs firmware on a real device connected to AWS IoT via a self-hosted GitHub Actions runner",
  "type": "module",
  "scripts": {
    "build": "esbuild run.ts --platform=node --bundle --outfile=build/run.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-aws-firmware-ci-feature-runner-action#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@actions/core": "1.5.0",
    "chalk": "4.1.2",
    "node-fetch": "3.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.1.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.1.4",
    "@types/node": "16.7.10",
    "@types/node-fetch": "3.0.3",
    "@types/uuid": "8.3.1",
    "esbuild": "0.13.0",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "7.24.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
